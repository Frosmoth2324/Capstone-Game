script/source = "extends CharacterBody2D
@onready var walking: AnimatedSprite2D = $Walking
@onready var falling: AnimatedSprite2D = $Falling
@onready var collision_shape_2d: CollisionShape2D = $\"../Collisions/CollisionShape2D\"
@onready var playerscreamsound: AudioStreamPlayer2D = $playerscream
@onready var walkingsound: AudioStreamPlayer2D = $walking


var SPEED = 300.0
const JUMP_VELOCITY = -600.0

func _ready():
	falling.hide()
	
	
func _physics_process(delta: float) -> void:
	# Add the gravity.
	if not is_on_floor():
		velocity += get_gravity() * delta


	# Get the input direction and handle the movement/deceleration.
	# As good practice, you should replace UI actions with custom gameplay actions.
	var direction := Input.get_axis(\"Left\", \"Right\")
	if direction:
		velocity.x = direction * SPEED
	else:
		velocity.x = move_toward(velocity.x, 0, SPEED)
	
	move_and_slide()
	
	if direction > 0:
		walking.play()
		walking.flip_h = false
	elif direction <0:
		walking.flip_h = true
		walking.play()
	else:
		walking.pause()


